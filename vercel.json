{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/dist/(.*)",
            "dest": "/dist/$1"
        },
        {
            "src": "/api-docs",
            "dest": "app.js"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}