{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs/(.*\\.(?:css|js|png|ico|html|svg))$",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}